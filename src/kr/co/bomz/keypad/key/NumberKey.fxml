<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox ?>
<?import javafx.scene.layout.HBox ?>
<?import javafx.scene.layout.FlowPane ?>
<?import javafx.scene.control.Button ?>
<?import javafx.scene.control.Label ?>


<HBox xmlns:fx="http://javafx.com/fxml/1" 
		fx:controller="kr.co.bomz.keypad.key.NumberKeyController"
		stylesheets="kr/co/bomz/keypad/key/Key.css,kr/co/bomz/keypad/key/NumberKey.css">
		
	<VBox fx:id="numberBox">
		<FlowPane>
			<Button text="1" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="2" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="3" onAction="#handleButtonAction" focusTraversable="false"  />	
		</FlowPane>
		<FlowPane>
			<Button text="4" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="5" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="6" onAction="#handleButtonAction" focusTraversable="false"  />	
		</FlowPane>
		<FlowPane>
			<Button text="7" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="8" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="9" onAction="#handleButtonAction" focusTraversable="false"  />	
		</FlowPane>
		<FlowPane>
			<Button text="0" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="00" userData="00" onAction="#handleButtonAction" focusTraversable="false"  />
			<Button text="000" userData="000" onAction="#handleButtonAction" focusTraversable="false"  />
		</FlowPane>
		<FlowPane>
			<Label text="" styleClass="tmpMargin"/>
		</FlowPane>
		<FlowPane>
			<Button text="띄어쓰기" userData=" " onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="삭제" userData="del" onAction="#handleButtonAction" focusTraversable="false"  />
		</FlowPane>
	</VBox>
	
	<VBox fx:id="priceBox">
		<FlowPane>
			<Button text="4,000" userData="4000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="4,500" userData="4500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="5,000" userData="5000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
		</FlowPane>
		<FlowPane>
			<Button text="5,500" userData="5500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="6,000" userData="6000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="6,500" userData="6500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
		</FlowPane>
		<FlowPane>
			<Button text="7,000" userData="7000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="7,500" userData="7500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="8,000" userData="8000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
		</FlowPane>
		<FlowPane>
			<Button text="8,500" userData="8500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="9,000" userData="9000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="9,500" userData="9500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
		</FlowPane>
		<FlowPane>
			<Label text="" />
		</FlowPane>
		<FlowPane>
			<Button text="10,000" userData="11000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="10,500" userData="10500" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
			<Button text="11,000" userData="11000" onAction="#handleButtonAction" styleClass="span2" focusTraversable="false"  />
		</FlowPane>
	</VBox>
	
</HBox>
	

	