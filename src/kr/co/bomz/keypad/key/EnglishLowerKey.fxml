		<Button text="n" onAction="#handleButtonAction" focusTraversable="false"  />
		<Button text="m" onAction="#handleButtonAction" focusTraversable="false"  />
	</FlowPane>
	
	<FlowPane>
	<Label text="" styleClass="tmpMargin"/>
	</FlowPane>
	
	<FlowPane>
		<Button text="띄어쓰기" userData=" " onAction="#handleButtonAction" styleClass="span4" focusTraversable="false"  />
		<Button text="삭제" userData="del" onAction="#handleButtonAction" styleClass="span3" focusTraversable="false"  />
		<Button text="." onAction="#handleButtonAction" focusTraversable="false"  />
