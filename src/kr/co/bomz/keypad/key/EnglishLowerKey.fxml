		<Button text="Shift" userData="shift" fx:id="upperLockShiftBt" onMouseClicked="#handleShiftAction" styleClass="span2" focusTraversable="false"  />
		<Button text="z" onAction="#handleButtonAction" focusTraversable="false"  />
		<Button text="x" onAction="#handleButtonAction" focusTraversable="false"  />
		<Button text="c" onAction="#handleButtonAction" focusTraversable="false"  />
		<Button text="v" onAction="#handleButtonAction" focusTraversable="false"  />
